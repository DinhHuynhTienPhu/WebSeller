<!-- 
searching Bar Section
-->
<div class="well">
  <form class="form-horizontal">
    <div class="control-group">
      <label class="control-label" for="inputProductName">Product name <sup>*</sup></label>
      <div class="controls">
        <input type="text" id="inputProductName" placeholder="Product name" />
      </div>
    </div>
    <div class="control-group container">
      <label class="control-label">Product Type <sup>*</sup></label>
      <div class="controls">
        <select class="span1" name="Type">
          <option value="">-</option>
          <option value="1">Shoe</option>
          <option value="2">Clothe</option>
          <option value="3">Watch</option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label">Producer <sup>*</sup></label>
      <div class="controls">
        <select class="span1" name="Producer">
          <option value="">-</option>
          <option value="1">Samsung</option>
          <option value="2">Iphone</option>
          <option value="3"></option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label">Origin <sup>*</sup></label>
      <div class="controls">
        <select class="span1" name="Producer">
          <option value="">-</option>
          <option value="1">China</option>
          <option value="2">VietNam</option>
          <option value="3">American</option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label">Price<sup>*</sup></label>
      <div class="controls">
        <select class="span1" name="Price">
          <option value="">-</option>
          <option value="1">2000000-5000000</option>
          <option value="2">5000000-10000000</option>
          <option value="3">10000000-20000000</option>
        </select>
      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <input type="submit" name="SearchingTheProduct" value="Searching" class="exclusive shopBtn" />
      </div>
    </div>
  </form>
</div>
